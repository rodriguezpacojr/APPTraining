<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.training.training.AboutOf">

    <TextView
        android:id="@+id/txtaboutOf"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="About Of..."
        android:layout_centerInParent="true"
        android:textSize="25dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_marginTop="25dp"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/txtnameDev"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="@string/strnameDev"
        android:textAlignment="center"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/txtaboutOf"
        android:layout_alignLeft="@+id/txtaboutOf"
        android:layout_alignStart="@+id/txtaboutOf"/>

    <TextView
        android:id="@+id/txtwebTec"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="@string/strwebTec"
        android:textAlignment="center"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/txtnameDev"
        android:layout_alignLeft="@+id/txtnameDev"
        android:layout_alignStart="@+id/txtnameDev"/>

    <TextView
        android:id="@+id/txtemailDev"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="@string/stremailDev"
        android:textAlignment="center"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/txtwebTec"
        android:layout_alignLeft="@+id/txtwebTec"
        android:layout_alignStart="@+id/txtwebTec"/>

    <TextView
        android:id="@+id/txtphoneDev"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="@string/strphoneDev"
        android:textAlignment="center"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/txtemailDev"
        android:layout_alignLeft="@+id/txtemailDev"
        android:layout_alignStart="@+id/txtemailDev"/>
</RelativeLayout>